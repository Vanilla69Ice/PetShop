<Window x:Class="PetShop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PetShop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="55*"/>
            <RowDefinition Height="154*"/>
            <RowDefinition Height="30*"/>
        </Grid.RowDefinitions>
        <Image Source="Resources/Логотип.jpg" HorizontalAlignment="Left" Margin="227,0,0,5" Width="103"/>
        <TextBlock Text="PetShop" FontSize="30" HorizontalAlignment="Center"></TextBlock>
        <Grid Background="Aquamarine" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="DarkViolet"></Grid>
        <Frame  NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name="MainFrame"></Frame>
        <Button Content="Back" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click"></Button>
        




    </Grid>
</Window>
